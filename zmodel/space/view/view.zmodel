
import "../../schema.zmodel"
import "../space.zmodel"
import "../../application"


model View extends IdCreatedUpdatedAuth {
    subTab   SubTabFolder @relation(fields: [subTabId], references: [id], onDelete: Cascade)
    subTabId String

    table    Type

    @@allow('read', subTab.tab.folder.applicationVersion.activations?[spaceId in auth().currentSpaceIds && space.profiles?[profile.users?[user == auth() && (profile.role == ADMIN || profile.role == USER)]]])
}
